<%= search_form_for @q do |f| %>
    <div class="row justify-content-end">
        <div class="form-group col-2">
            <%= f.label "" %>
        </div>

        <div class="form-group col-2">
            <%= f.label "" %>
            <%= f.select :date_eq, options_for_select(months_list), {include_blank: 'MÃªs da Consulta'}, class: 'form-control' %>
        </div>

        <div class="form-group col-2">
            <%= f.label "" %>
            <%= f.collection_select(:cnes_unity_cont, Cnes.all.order(:unity), :unity, :unity, {include_blank: 'Unidade'}, class: 'form-control') %>
        </div>

        <div class="form-group col-3">
            <%= f.label "" %>
            <%= f.search_field :patient_name_cont, class: 'form-control', placeholder: 'Nome do paciente' %>
        </div> 

        <div class="form-group pt-4 mr-3">
            <%= f.submit 'Buscar', class: 'btn btn-primary shadow-sm'  %>
        </div>
    </div>
<% end %>